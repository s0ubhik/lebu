func kol(Duck d) {
	d.name = "Mukesh";
	d.size = 20;
	return d;
}


class Duck {
	str name;
	int size;

	func _init(name, size) {
		this.name = name;
		this.size = size;
	}

	func quack(this) {
		printf("%d Quack, Quack %s\n", this.size, this.name);
	}
}

duck = Duck("Soubhik", 10);
duck.quack();

duck = kol(duck);
duck.quack();

